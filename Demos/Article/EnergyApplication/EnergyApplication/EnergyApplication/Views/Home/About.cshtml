<script type="text/javascript">
    $(function () {

        var frm = $('#formId');
        var button = $('#buttonAsync');
        button.click(function () {
            $.ajax({
                type: frm.attr('method'),
                url: '/Home/AllFuelsInTime',
                data: frm.serialize(),
                success: function (data) {
                    $('#chart1').empty().append(data);
                    chartFunction();
                }
            });
        });
    });
</script>


