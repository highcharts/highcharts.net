@*You need to add a reference to jQuery prior to referencing the highcharts javascript file*@
<script src="http://code.highcharts.com/highcharts.js"></script>
<script src="https://code.highcharts.com/modules/exporting.js"></script>

@using Highsoft.Web.Mvc.Charts
@using System.Collections.Specialized
@using System.Collections
   
    @(Html.Highsoft().Highcharts(
        new Highcharts
        {
            Title = new Title
            {
                Text = "Snow depth at Vikjafjellet, Norway"
            },
            Subtitle = new Subtitle
            {
                Text = "Irregular time data in Highcharts JS",
            },
            XAxis = new List<XAxis>
            {
                new XAxis
                {
                    Type = XAxisType.Datetime,
                    DateTimeLabelFormats = new Hashtable
                    {
                        //{"day","%e. %b" }
                        { "month" , "%e. %b" },
                        { "year" , "%b" }
                    },
                    Title = new XAxisTitle
                    {
                        Text = "Date"
                    }
                }
            },
            YAxis = new List<YAxis>
            {
                new YAxis
                {
                    Title = new YAxisTitle
                    {
                        Text = "Snow depth (m)"
                    },
                    Min = 0
                }
            },
            Tooltip = new Tooltip
            {
                HeaderFormat = "<b>{series.name}</b><br>",
                PointFormat = "{point.x:%e. %b}: {point.y:.2f} m"
            },
            PlotOptions = new PlotOptions
            {
                Spline = new PlotOptionsSpline
                {
                    Marker = new PlotOptionsSplineMarker
                    {
                        Enabled = true
                    }
                }
            },
            Series = new List<Series>
            {
                new SplineSeries
                {
                    Name = "Winter 2012-2013",
                    Data = @ViewData["winter1"] as List<SplineSeriesData>
                },
                new SplineSeries
                {
                    Name = "Winter 2013-2014",
                    Data = @ViewData["winter2"] as List<SplineSeriesData>
                },
                new SplineSeries
                {
                    Name = "Winter 2014-2015",
                    Data = @ViewData["winter3"] as List<SplineSeriesData>
                }
            }
        }
        , "chart")
    )