<script src="http://code.highcharts.com/highcharts.js"></script>
<script src="https://code.highcharts.com/modules/exporting.js"></script>

@using Highsoft.Web.Mvc.Charts
@using Highsoft.Web.Mvc.Charts.Rendering

@{ var chartOptions = new Highcharts
    {
        Title = new Title
        {
            Text = "Stacked bar chart"
        },
        XAxis = new List<XAxis>
    {
                new XAxis
                {
                    Categories = new List<string> { "Apples", "Oranges", "Pears", "Grapes", "Bananas" }
                }
            },
        YAxis = new List<YAxis>
    {
                new YAxis
                {
                    Min = 0,
                    Title = new YAxisTitle
                    {
                        Text = "Total fruit consumption"
                    }
                }
            },
        Legend = new Legend
        {
            Reversed = true
        },
        PlotOptions = new PlotOptions
        {
            Series = new PlotOptionsSeries
            {
                Stacking = PlotOptionsSeriesStacking.Normal
            }
        },
        Series = new List<Series>
    {
                new BarSeries
                {
                    Name = "John",
                    Data = @ViewData["johnData"] as List<BarSeriesData>
                },
                new BarSeries
                {
                    Name = "Jane",
                    Data = @ViewData["janeData"] as List<BarSeriesData>
                },
                new BarSeries
                {
                    Name = "Joe",
                    Data = @ViewData["joeData"] as List<BarSeriesData>
                }
            }
    };

    chartOptions.ID = "chart";
    var renderer = new HighchartsRenderer(chartOptions);
}

@Html.Raw(renderer.RenderHtml());